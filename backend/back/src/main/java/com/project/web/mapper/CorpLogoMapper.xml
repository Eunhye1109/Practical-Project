<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.project.web.mapper.CorpLogoMapper">

<select id="findLogoUrlByName" parameterType="string" resultType="string">
  SELECT LOGO_URL
  FROM   ${schema}.CORP_LOGO
  WHERE  CORP_NAME = #{name}
</select>


</mapper>
